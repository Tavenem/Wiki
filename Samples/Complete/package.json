{
  "name": "neverfoundrywikisamplescomplete",
  "private": true,
  "version": "0.1.0",
  "devDependencies": {
    "bootstrap": "^4.5.2",
    "cssnano": "^4.1.10",
    "postcss-cli": "^7.1.1",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.26.10"
  },
  "scripts": {
    "scss": "sass --load-path=node_modules/bootstrap/scss --load-path=../../Web/Mvc/wwwroot --style=compressed ./content/styles/site.scss ./wwwroot/site.css",
    "postcss": "postcss ./wwwroot/site.css -m -r --config .",
    "build": "npm run scss && npm run postcss"
  }
}
