{
  "Elasticsearch": {
    "Url": "http://elasticsearch:9200",
    "Index": "article"
  },
  "EmailConfiguration": {
    "FromAddress": "noreply@neverfoundry.com",
    "FromName": "NeverFoundry",
    "ReplyToAddress": "noreply@neverfoundry.com"
  },
  "fido2": {
    "MDSAccessKey": null,
    "serverDomain": "localhost",
    "origin": "http://localhost:5000",
    "TimestampDriftTolerance": 300000
  },
  "Serilog": {
    "LogEventLevel": 3,
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "Site": {
    "SiteName": "NeverFoundry Sample",
    "SiteURL": "https://localhost:5001"
  }
}
