{
  "name": "neverfoundrywikisamples",
  "private": true,
  "version": "0.1.0",
  "devDependencies": {
    "bootstrap": "^4.5.2",
    "cssnano": "^4.1.10",
    "ncp": "^2.0.0",
    "postcss-cli": "^7.1.1",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.26.10"
  },
  "scripts": {
    "scss": "sass --load-path=node_modules/bootstrap/scss --load-path=../../Web/Mvc/wwwroot --style=compressed ./styles/site.scss ../Complete/wwwroot/site.css && ncp ../Complete/wwwroot/site.css ../Simple/wwwroot/site.css && ncp ../Complete/wwwroot/site.css.map ../Simple/wwwroot/site.css.map",
    "postcss": "postcss ../Complete/wwwroot/site.css ../Simple/wwwroot/site.css -m -r --config .",
    "build": "npm run scss && npm run postcss"
  }
}
